    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.5.0",
    "react-draggable": "^2.1.2",
    "react-hot-loader": "^4.3.6",
    "react-redux": "^4.0.1",
    "react-resizable": "^1.4.2",
    "react-router": "~4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^1.3.0",
    "react-tabs": "git+https://github.com/maxcnunes/react-tabs.git#remove-warnings",
