    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4",

    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "uglifyjs-webpack-plugin": "^1.3.0"
  },
  "engines": {
    "node": ">=6"
  }
}
