{
  "name": "@armarti/unified-dataloader-gui",
  "version": "0.0.1",
  "license": "GPL-3.0-only",
  "description": "@armarti/unified-dataloader-gui",
  "author": "Armarti Industries",
  "contributors": [
    "David Charles Morse <dcmorse@armarti.com>",
    "The SQLECTRON Team <maxcnunes@gmail.com>"
  ],
  "keywords": [],
  "bugs": "https://github.com/armarti/unified-dataloader-gui/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/armarti/unified-dataloader-gui"
  },
  "build": {
    "appId": "org.unified-dataloader.gui",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "dmg": {
      "contents": [
        {
          "x": 438,
          "y": 275,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 275,
          "type": "file"
        }
      ]
    },
    "linux": {
      "depends": [],
      "target": [
        "deb",
        "rpm",
        "sh",
        "freebsd",
        "pacman",
        "tar.xz",
        "tar.gz"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "7z",
        "zip"
      ]
    }
  },
  "main": "./out/browser/main.js",
  "scripts": {
    "postinstall": "electron-rebuild",
    "dev": "concurrently --names 'webpack,electron' --prefix name 'yarn run dev:webpack' 'yarn run dev:electron'",
    "dev:webpack": "webpack-dev-server --color --config ./webpack.dev.config.js",
    "dev:electron": "cross-env DEBUG=spawn-auto-restart node scripts/dev-auto-restart.js | bunyan -o short",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "yarn run lint -- --fix",
    "test": "yarn run lint",
    "clean": "rimraf dist",
    "clean:osx": "rimraf dist/Sqlectron-darwin-*",
    "clean:win": "rimraf dist/win* dist/Sqlectron-win*",
    "clean:linux": "rimraf dist/Sqlectron-linux-* dist/*.deb",
    "compile": "rimraf out && yarn run compile:browser && yarn run compile:renderer",
    "compile:browser": "cross-env NODE_ENV=production babel ./src/browser -d out/browser",
    "compile:renderer": "cross-env NODE_ENV=production webpack --progress --display-error-details --config ./webpack.prod.config",
    "build:osx": "yarn run clean:osx && build --osx",
    "build:linux": "yarn run clean:linux && build --linux",
    "build:win": "yarn run clean:win && build --windows",
    "dist": "yarn run compile && build",
    "dist:all-arch": "yarn run compile && build --ia32 --x64",
    "dist:winlinux": "yarn run compile && yarn run build:linux && yarn run build:win"
  },
  "dependencies": {
    "@armarti/unified-dataloader-core": "latest",
    "@babel/core": "^7.0.1",
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.7.0",
    "bunyan": "^1.8.5",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "~8.0.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-preset-react-hmre": "^1.1.1",
    "brace": "^0.7.0",
    "breakpoint-sass": "^2.6.1",
