{
  "name": "@armarti/unified-dataloader-gui",
  "version": "0.0.1",
  "license": "GPL-3.0-only",
  "description": "@armarti/unified-dataloader-gui",
  "author": "Armarti Industries",
  "contributors": [
    "David Charles Morse <dcmorse@armarti.com>",
    "The SQLECTRON Team <maxcnunes@gmail.com>"
  ],
  "keywords": [],
  "bugs": "https://github.com/armarti/unified-dataloader-gui/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/armarti/unified-dataloader-gui"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=6 <10.2",
    "yarn": ">=1.0"
  },
  "build": {
    "electronVersion": "3.0.0",
    "appId": "org.unified-dataloader.gui",
    "productName": "unified-dataloader",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "dmg": {
      "contents": [
        {
          "x": 438,
          "y": 275,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 275,
          "type": "file"
        }
      ]
    },
    "linux": {
      "depends": [],
      "target": [
        "deb",
        "rpm",
        "sh",
        "freebsd",
        "pacman",
        "tar.xz",
        "tar.gz"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "7z",
        "zip"
      ]
    }
  },
  "main": "./out/browser/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "dev": "concurrently --names 'webpack,electron' --prefix name 'yarn run dev:webpack' 'yarn run dev:electron'",
    "dev:full": "concurrently --names 'databases,devtools,webpack,electron' --prefix name 'yarn run dev:databases' 'yarn run dev:devtools' 'yarn run dev:webpack' 'yarn run dev:electron'",
    "databases": "docker-compose --file ../../../sqlectron-databases/docker-compose.yml up",
    "dev:devtools": "yarn run react-devtools",
    "dev:webpack": "cross-env BABEL_CACHE_PATH=./cache/babel-dev-webpack.json node --trace-warnings --require @babel/register node_modules/.bin/webpack-dev-server --color --config=config/webpack/dev.js",
    "dev:electron": "cross-env DEBUG=spawn-auto-restart node --trace-warnings --require @babel/register scripts/dev-auto-restart.js | bunyan -o short",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "yarn run lint -- --fix",
    "test": "yarn run lint",
    "clean": "rimraf dist",
    "clean:osx": "rimraf dist/unified-dataloader-gui-darwin-*",
    "clean:win": "rimraf dist/win* dist/unified-dataloader-gui-win*",
    "clean:linux": "rimraf dist/unified-dataloader-gui-linux-* dist/*.deb",
    "compile": "rimraf out && yarn run compile:browser && yarn run compile:renderer",
    "compile:browser": "cross-env BABEL_CACHE_PATH=./cache/babel-compile-browser.json NODE_ENV=production node --trace-warnings node_modules/.bin/babel ./src/browser -d out/browser",
    "compile:renderer": "cross-env BABEL_CACHE_PATH=./cache/babel-compile-renderer.json node --trace-warnings --require @babel/register node_modules/.bin/webpack --progress --display-error-details --config=config/webpack/prod.js",
    "build:osx": "yarn run clean:osx && build --osx",
    "build:linux": "yarn run clean:linux && build --linux",
    "build:win": "yarn run clean:win && build --windows",
    "dist": "yarn run compile && build",
    "dist:all-arch": "yarn run compile && build --ia32 --x64",
    "dist:winlinux": "yarn run compile && yarn run build:linux && yarn run build:win"
  },
  "dependencies": {
    "@armarti/unified-dataloader-core": "*",
    "@babel/runtime": "^7.0.0",
    "axios": "^0.7.0",
    "bunyan": "^1.8.5",
    "electron": "^3.0.0",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "react": "^15.0",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-select": "^2.0.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "~8.0.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "brace": "^0.7.0",
    "breakpoint-sass": "^2.6.1",
    "classnames": "^2.2.5",
    "concurrently": "^2.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "csv-stringify": "0.0.8",
    "del": "^2.2.0",
    "denodeify": "^1.2.1",
    "dom-helpers": "^3.3.1",
    "electron-builder": "^20.28.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "html2canvas": "^0.5.0-beta4",
    "image-webpack-loader": "4.3.1",
    "imports-loader": "^0.6.5",
    "jointjs": "^0.9.9",
    "jquery": "^3.3.1",
    "json-loader": "^0.5.4",
    "mini-css-extract-plugin": "^0.4.2",
    "minimist": "^1.2.0",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "react-ace": "~6.1.4",
    "react-addons-shallow-compare": "^15.1.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15",
    "react-draggable": "^2.1.2",
    "react-hot-loader": "^4.3.6",
    "react-redux": "^4.0.1",
    "react-resizable": "^1.4.2",
    "react-router-dom": "^4.3.1",
    "react-select": "^1.3.0",
    "react-tabs": "git+https://github.com/maxcnunes/react-tabs.git#remove-warnings",
    "react-transform-hmr": "^1.0.1",
    "react-virtualized": "^9.20.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "spawn-auto-restart": "^2.0.1",
    "sqlite3": "^4.0.2",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.3.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.18.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  },
  "optionalDependencies": {
    "react-devtools": "^3.3.4"
  }
}
